{
	"env": {
		"node": true
	},
	"rules": {
		"camelcase": 2,
		"consistent-return": 0,
		"key-spacing": 0,
		"new-cap": 0,
		"no-alert": 0,
		"no-caller": 2,
		"no-multi-spaces": 0,
		"no-shadow": 0,
		"no-undef": 2,
		"no-underscore-dangle": 1,
		"no-unused-vars": 2,
		"no-use-before-define": [2, "nofunc"],
		"quotes": [1, "single"],
		"semi": 2,
		"space-after-keywords": 2,
		"space-before-function-paren": 1,
		"space-before-keywords": [2, "always"],
		"strict": 2
	},
	"ecmaFeatures": {
		"arrowFunctions": true,
		"binaryLiterals": true,
		"blockBindings": true,
		"classes": true,
		"defaultParams": true,
		"destructuring": true,
		"forOf": true,
		"generators": true,
		"globalReturn": true,
		"modules": true,
		"objectLiteralComputedProperties": true,
		"objectLiteralDuplicateProperties": true,
		"objectLiteralShorthandMethods": true,
		"objectLiteralShorthandProperties": true,
		"octalLiterals": true,
		"regexUFlag": true,
		"regexYFlag": true,
		"restParams": true,
		"spread": true,
		"superInFunctions": true,
		"templateStrings": true,
		"unicodeCodePointEscapes": true
	},
	"globals": {
		"Promise": true
	}
}